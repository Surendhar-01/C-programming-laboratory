#include<stdio.h>
int main()
{
    int n,sum=0,product=1;
    float average;
    printf("n=");
    scanf("%d",&n);
    for(int i=1;i<=n;i++)
    {
        printf("%d\n",i);
    }
    for(int i=1;i<=n;i++)
    {
           sum=sum+i;
           product=product*i;
    }
    average=(float)sum/n;
    printf("%d\n%d\n%f",sum,product,average);
    return 0;
}
